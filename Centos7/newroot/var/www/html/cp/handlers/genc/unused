sub print_intre {

my %Coo = TClubMD5::GetCookies('IQS');
my $i_coo = defined($Coo{'IQS'}) ? $Coo{'IQS'} : '';

print qq {

<script>
function clearCookie(cname,value) {
	var curCookie = escape(cname) + "=" + escape(value) +"; expires=Mon, 01-Jan-1970 00:00:01 GMT;"
	document.cookie = curCookie
}
clearCookie('IQS','');
</script>

<script>
location.href = '/?$i_coo';
</script>
};

}

